{
  "Author": "wpz",
  "Routes": [
    {
      "ServiceName": "Demo",
      // https://regexr.com/
      // 简化版，只替换开头
      "UpstreamPathTemplate": "/myservice/",
      // try: http://localhost:5100/api/Categories/1/children
      "DownstreamPathTemplate": "/api/",
      // might load balance
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "5100"
        }
      ],
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "ServiceName": "Demo2",
      "UpstreamPathTemplate": "/netapp/",
      "DownstreamPathTemplate": "/",
      // might load balance
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "5000"
        }
      ],
      "UpstreamHttpMethod": [ "GET" ],
      "CacheTtlSeconds": 60
    }
  ]
}
