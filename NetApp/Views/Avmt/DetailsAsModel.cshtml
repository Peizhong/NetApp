@model IEnumerable<NetApp.Entities.Avmt.FunctionLocation>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DetailsAsModel</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FlCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SortNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkspaceId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProvinceCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BureauCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ObjectType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClassifyId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VoltageId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.State)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FlCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SortNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WorkspaceId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProvinceCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BureauCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ObjectType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClassifyId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VoltageId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.State)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
